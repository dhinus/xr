{
  "name": "xr",
  "version": "0.2.0",
  "description": "Ultra-simple wrapper around XMLHttpRequest.",
  "main": "xr.js",
  "scripts": {
    "update-authors": "git log --format='%aN <%aE>' | sort -u | grep -v noreply > AUTHORS",
    "prepublish": "npm run build; npm run update-authors",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "babver": "babel --version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radiosilence/xr"
  },
  "keywords": [
    "request",
    "xmlhttprequest",
    "json",
    "ajax"
  ],
  "author": "James Cleveland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radiosilence/xr/issues"
  },
  "homepage": "https://github.com/radiosilence/xr",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "webpack": "^1.12.12"
  },
  "dependencies": {
    "querystring": "^0.2.0"
  }
